@model IEnumerable<ShenDeng.Framework.Domain.Account>
@{
    ViewBag.Title = "ManageAccount";
}

<table border="1">
    <tr>
        <th>
            用户名
        </th>
        <th>
            权限
        </th>

    </tr>
    
    @foreach (var a in Model)
    {
        <tr>
            <td>
                @a.Id.UserName
            </td>
            <td>
                @a.Roles
            </td>
            <td>
                @Html.ActionLink("删除","Delete_Account", new {id = a.Id.UserName})
            </td>
        </tr>
    }
</table>

